{
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
  
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
  
    "globals": {
      "document": false,
      "navigator": false,
      "window": false
    },
  
    "plugins": [
        "json"
    ],
  
    "rules": {
      "comma-dangle": ["error","never"],
      "no-cond-assign": [2,"always"],
      "no-console": 1,
      "no-constant-condition": 2,
      "no-control-regex": 2,
      "no-debugger": 1,
      "no-dupe-args": 1,
      "no-dupe-keys": 1,
      "no-duplicate-case": 1,
      "no-empty-character-class": 1,
      "no-empty": 1,
      "no-ex-assign": 1,
      "no-extra-boolean-cast": 1,
      "no-extra-parens": [1,"all"],
      "no-extra-semi": 1,
      "no-func-assign": 1,
      "no-inner-declarations": [1,"both"],
      "no-invalid-regexp": 1,
      "no-irregular-whitespace": 1,
      "no-negated-in-lhs": 1,
      "no-obj-calls": 1,
      "no-regex-spaces": 1,
      "no-sparse-arrays": 1,
      "no-unreachable": 1,
      "use-isnan": 1,
      "valid-jsdoc": [1,{"prefer":{"arg":"param","argument":"param","class":"class","return":"returns","virtual":"abstract"},"requireReturn":true,"requireReturnType":true}],
      "valid-typeof": 2,
      "no-unexpected-multiline": 0,
      "accessor-pairs": 0,
      "block-scoped-var": 0,
      "complexity": 0,
      "consistent-return": 0,
      "curly": [1,"multi-line"],
      "default-case": 1,
      "dot-notation": 1,
      "dot-location": [1,"property"],
      "eqeqeq": [1,"smart"],
      "guard-for-in": 0,
      "no-alert": 1,
      "no-caller": 1,
      "no-div-regex": 1,
      "no-else-return": 1,
      "no-eq-null": 1,
      "no-eval": 1,
      "no-extend-native": 0,
      "no-extra-bind": 1,
      "no-fallthrough": 1,
      "no-floating-decimal": 1,
      "no-implicit-coercion": 0,
      "no-implied-eval": 1,
      "no-invalid-this": 1,
      "no-iterator": 0,
      "no-labels": 1,
      "no-lone-blocks": 1,
      "no-loop-func": 0,
      "no-multi-spaces": 0,
      "no-multi-str": 0,
      "no-native-reassign": 0,
      "no-new-func": 0,
      "no-new-wrappers": 0,
      "no-new": 0,
      "no-octal-escape": 0,
      "no-octal": 0,
      "no-param-reassign": 0,
      "no-process-env": 0,
      "no-proto": 0,
      "no-redeclare": 0,
      "no-return-assign": 0,
      "no-script-url": 0,
      "no-self-compare": 0,
      "no-sequences": 0,
      "no-throw-literal": 0,
      "no-unused-expressions": 0,
      "no-useless-call": 0,
      "no-void": 0,
      "no-warning-comments": 0,
      "no-with": 0,
      "radix": 0,
      "vars-on-top": 0,
      "wrap-iife": 0,
      "yoda": 0,
      "strict": 0,
      "init-declarations": 0,
      "no-catch-shadow": 0,
      "no-delete-var": 1,
      "no-label-var": 0,
      "no-shadow-restricted-names": 0,
      "no-shadow": [1,{"hoist":"all"}],
      "no-undef-init": 0,
      "no-undef": 1,
      "no-undefined": 1,
      "no-unused-vars": 1,
      "no-use-before-define": 0,
      "callback-return": 1,
      "handle-callback-err": 0,
      "no-mixed-requires": 0,
      "no-new-require": 1,
      "no-path-concat": 1,
      "no-process-exit": 1,
      "no-restricted-modules": 0,
      "no-sync": 1,
      "array-bracket-spacing": 0,
      "brace-style": 0,
      "camelcase": [1,{"properties":"always"}],
      "comma-spacing": [1,{"after":true}],
      "comma-style": 0,
      "computed-property-spacing": 0,
      "consistent-this": 0,
      "eol-last": 1,
      "func-names": 0,
      "func-style": 0,
      "id-length": 0,
      "indent": ["error", 2],
      "key-spacing": [1,{"align":"colon","afterColon":true}],
      "lines-around-comment": 0,
      "linebreak-style": [2,"unix"],
      "max-nested-callbacks": 0,
      "new-cap": 0,
      "new-parens": 0,
      "newline-after-var": 0,
      "no-array-constructor": 0,
      "no-continue": 0,
      "no-inline-comments": 0,
      "no-lonely-if": 0,
      "no-mixed-spaces-and-tabs": 2,
      "no-multiple-empty-lines": [1,{"max":1}],
      "no-nested-ternary": 0,
      "no-new-object": 0,
      "no-spaced-func": 0,
      "no-ternary": 0,
      "no-trailing-spaces": 1,
      "no-underscore-dangle": 1,
      "no-unneeded-ternary": 0,
      "object-curly-spacing": [1,"always",{}],
      "one-var": 0,
      "operator-assignment": 0,
      "operator-linebreak": 0,
      "padded-blocks": 0,
      "quote-props": 0,
      "quotes": ["warn", "single", { "allowTemplateLiterals": true }],
      "id-match": 0,
      "semi-spacing": 0,
      "semi": 0,
      "sort-vars": 0,
      "space-after-keywords": 0,
      "space-before-blocks": 0,
      "space-before-function-paren": 0,
      "space-in-parens": 0,
      "space-infix-ops": 0,
      "space-return-throw-case": 0,
      "space-unary-ops": 0,
      "spaced-comment": 0,
      "wrap-regex": 0,
      "arrow-parens": 0,
      "arrow-spacing": [1,{"before":true,"after":true}],
      "constructor-super": 0,
      "generator-star-spacing": 1,
      "no-class-assign": 0,
      "no-const-assign": 0,
      "no-this-before-super": 2,
      "no-var": 1,
      "object-shorthand": 0,
      "prefer-const": 1,
      "prefer-spread": 1,
      "prefer-reflect": 1,
      "require-yield": 1,
      "max-depth": 0,
      "max-len": [1,{"code":120,"tabWidth":2,"ignoreComments":true,"ignoreTrailingComments": true,"ignoreTemplateLiterals": true}],
      "max-params": ["warn", 5],
      "max-statements": ["warn", 10, { "ignoreTopLevelFunctions": true }],
      "no-bitwise": 0,
      "no-plusplus": 0
    }
  }